{
  "name": "wp/pansionat-theme",
  "type": "project",
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.13",
    "wp-coding-standards/wpcs": "^3.2",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.1"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "PancionatTheme\\": "inc/"
    }
  },
  "scripts": {
    "lint": "vendor/bin/phpcs --standard=WordPress --extensions=php --ignore=vendor,assets,languages --exclude=WordPress.Commenting.FileComment,WordPress.Commenting.FunctionComment,WordPress.Files.FileName,WordPress.Commenting.ClassComment,Squiz.Commenting.FileComment,Squiz.Commenting.FunctionComment,Squiz.Commenting.ClassComment .",
    "fix": "vendor/bin/phpcbf --standard=WordPress --extensions=php --ignore=vendor,assets,languages --exclude=WordPress.Commenting.FileComment,WordPress.Commenting.FunctionComment,WordPress.Files.FileName,WordPress.Commenting.ClassComment,Squiz.Commenting.FileComment,Squiz.Commenting.FunctionComment,Squiz.Commenting.ClassComment ."
  },
  "require": {
    "enshrined/svg-sanitize": "^0.22.0"
  }
}
